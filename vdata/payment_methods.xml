<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <!-- bKash -->
    <record id="payment_method_bkash" model="payment.method">
        <field name="code">bkash</field>
        <field name="name">bKash</field>
        <field name="sequence">1001</field>
        <field name="support_tokenization">False</field>
        <field name="support_express_checkout">False</field>
        <field name="supported_country_ids"
            eval="[Command.set([ref('base.bd')])]"/>
        <field name="supported_currency_ids"
            eval="[Command.set([ref('base.BDT')])]"/>
    </record>

</odoo>
